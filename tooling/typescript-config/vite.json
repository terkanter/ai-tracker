{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Vite React",
  "extends": "./base.json",
  "compilerOptions": {
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "verbatimModuleSyntax": true,
    "noEmit": true,
    "jsx": "react-jsx"
  }
}
