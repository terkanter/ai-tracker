{
  "name": "ai-tracker",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "dev:web": "pnpm --filter ./apps/web dev",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "lint:unsafe": "biome check . --write --unsafe",
