    "check:unsafe": "biome check . --write --unsafe",
    "type-check": "turbo type-check"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "turbo": "^2.3.0"
  },
  "packageManager": "pnpm@9.12.3",
  "engines": {
    "node": ">=20"
  }
}
